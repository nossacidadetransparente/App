{
  "name": "app",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.jsx",
  "scripts": {
    "prepare": "rm -rf dist",
    "start": "npm run prepare && webpack-dev-server",
    "build": "npm run prepare && env PROD_ENV=1 webpack",
    "test:features": "env NODE_TLS_REJECT_UNAUTHORIZED=0 cucumber.js",
    "test:unit": "jest",
    "test": "npm run test:unit",
    "posttest": "npm run lint",
    "lint": "npm run lintjs && npm run lintcss",
    "lintjs": "./node_modules/.bin/eslint --ext jsx --ext js .",
    "lintcss": "scss-lint src" 
  },
  "jest": {
    "moduleNameMapper": {
      "^.*\\.scss$": "<rootDir>/test/util/scss-preprocessor.js"
    },
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/react-dom/",
      "node_modules/react-addons-test-utils/",
      "node_modules/react-redux/",
      "node_modules/redux",
      "node_modules/redux-thunk",
      "node_modules/redux-mock-store",
      "node_modules/enzyme/",
      "node_modules/axios"
    ]
  },
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-jest": "^12.1.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "cucumber": "^1.0.0",
    "hapi": "^13.4.0",
    "inert": "^4.0.0",
    "nightmare": "^2.5.1",
    "enzyme": "^2.3.0",
    "eslint": "^2.9.0",
    "eslint-config-fringe": "0.0.8",
    "eslint-import-resolver-webpack": "^0.2.4",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-jsx-a11y": "^1.0.4",
    "eslint-plugin-perf-standard": "^1.0.3",
    "eslint-plugin-react": "^5.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.17.0",
    "imports-loader": "^0.6.5",
    "jest-cli": "^12.1.1",
    "node-sass": "^3.7.0",
    "react-addons-test-utils": "^15.1.0",
    "react-hot-loader": "^1.3.0",
    "redux-mock-store": "^1.1.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
